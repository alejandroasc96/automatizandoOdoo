<odoo>
  <data>
    <!-- explicit list view definition -->

    <record id="InventiaPlusPaper" model="report.paperformat">
      <field name="name">InventiaPlusPaper</field>
      <field name="default" eval="True" />
      <field name="format">A4</field>
      <field name="page_height">0</field>
      <field name="page_width">0</field>
      <field name="orientation">Portrait</field>
      <field name="margin_top">55</field>
      <field name="margin_bottom">27</field>
      <field name="margin_left">0</field>
      <field name="margin_right">0</field>
      <field name="header_line" eval="False" />
      <field name="header_spacing">70</field>
      <field name="dpi">90</field>
    </record>

    <!-- schedule task -->
    <record forcecreate="True" id="check_expired_services" model="ir.cron">
      <field name="name">Check Expired Services</field>
      <field eval="True" name="active" />
      <field name="user_id" ref="base.user_root" />
      <field name="interval_number">24</field>
      <field name="interval_type">hours</field>
      <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 05:00:00')" />
      <field name="numbercall">-1</field>
      <field ref="model_account_invoice_line" name="model_id" />
      <field name="state">code</field>
      <field name="code">model._checkAllServices()</field>
      <field eval="False" name="doall"/>
      <field name="function">True</field>
    </record>

    <record model="ir.ui.view" id="Gestion_de_Dominios_Alojamientos.form">
      <field name="name">Gestion_de_Dominios_Alojamientos_form</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="product.product_template_form_view"/>
      <field name="arch" type="xml">
        <xpath expr="//page[@name='general_information']/group/group[@name='group_standard_price']">
          <field name="dead_line_service"/>
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="Gestion_de_Dominios_Alojamientos.list">
      <field name="name">Gestion_de_Dominios_Alojamientos_list</field>
      <field name="model">account.invoice.line</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="invoice_id"/>
          <field name="date_to_end"/>
          <field name="create_date"/>
          <field name="days_to_end"/>
          <field name="product_id"/>
        </tree>
      </field>
    </record>

    <!-- vista calendario -->
    <record model="ir.ui.view" id="Gestion_de_Dominios_Alojamientos.calendar">
      <field name="name">Gestion_de_Dominios_Alojamientos_calendar</field>
      <field name="model">account.invoice.line</field>
      <field name="arch" type="xml">
        <calendar string="date_to_end" date_start="date_to_end">
          <field name="invoice_id"/>
          <field name="partner_id"/>
          <field name="product_id"/>
        </calendar>
      </field>
    </record>

    <record model="mail.channel" id="channel_sb_1">
      <field name="name">Denied Orders</field>
      <field name="public">public</field>
      <field name="description">Medical refill information details.</field>
    </record>



    <record id="deadLine_service_form" model = "ir.ui.view">
      <field name="name">service_line</field>
      <field name="model">account.invoice</field>
      <field name = "inherit_id" ref="account.invoice_form"/>
      <field name="arch" type="xml">
        <xpath expr="//notebook/page/field/tree" position="inside">
          <field name="date_to_end"/>
          <field name="create_date"/>
          <field name="days_to_end"/>
        </xpath>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="Gestion_de_Dominios_Alojamientos.action_window">
      <field name="name">Gestion_de_Dominios_Alojamientos</field>
      <field name="res_model">account.invoice.line</field>
      <field name="view_id" ref="Gestion_de_Dominios_Alojamientos.list"/>
      <field name="view_mode">tree,calendar</field>
    </record>


    <!-- server action to the one above -->
    <!--
    <record model="ir.actions.server" id="Gestion_de_Dominios_Alojamientos.action_server">
      <field name="name">Gestion_de_Dominios_Alojamientos server</field>
      <field name="model_id" ref="model_Gestion_de_Dominios_Alojamientos_Gestion_de_Dominios_Alojamientos"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>
    -->

    <!-- Top menu item -->

    <menuitem name="Gestion de Dominions/Alojamientos" id="Gestion_de_Dominios_Alojamientos.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Servicios" id="Gestion_de_Dominios_Alojamientos.menu_1" parent="Gestion_de_Dominios_Alojamientos.menu_root" action="Gestion_de_Dominios_Alojamientos.action_window"/>
    <!-- <menuitem name="Emails" id="Gestion_de_Dominios_Alojamientos.menu_2" parent="Gestion_de_Dominios_Alojamientos.menu_root" action="Gestion_de_Dominios_Alojamientos.action_window"/> -->

    <!-- <menuitem name="Server to list" id="Gestion_de_Dominios_Alojamientos" parent="Gestion_de_Dominios_Alojamientos.menu_2" action="Gestion_de_Dominios_Alojamientos.action_server"/> -->

  </data>
</odoo>